    "main": "./dist/index.js",
    "scripts": {
        "build:js": "npm run clean:lib && tsc && tsc-alias",
        "build:prod": "webpack",
        "eslint": "eslint ./src --fix",
        "dev:node": "pack2 --mode=dev:node --DOT_ENV=development",
        "dev:node2": "node --inspect-brk  --loader ts-node/esm ./src/index.ts",
        "dev:node:debug": "pack2 --mode=dev:node --DOT_ENV=development -d",
        "dev:node:debug2": "node --inspect-brk  --loader ts-node/esm ./src/index.ts",
        "inset:prompt:db": "pack2 --mode=dev:node --DOT_ENV=development  --config=./pack.script.cjs",
        "build:node": "pack2 --mode=build:node",
        "test": "jest --passWithNoTests --detectOpenHandles",
        "test:file": "node ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles __tests__/mongodb/users/prompt.ts",
        "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles __tests__/mongodb/users/prompt.ts",
        "jest:clean": "jest --clearCache"
    },
    "author": "",
    "license": "ISC",
